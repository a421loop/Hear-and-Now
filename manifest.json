{
  "manifest_version": 3,
  "name": "Prompt Timer",
  "version": "1.1",
  "description": "A new tab page with rotating writing prompts and timer functionality",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://docs.google.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "sounds/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
